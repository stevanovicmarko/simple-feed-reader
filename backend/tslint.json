{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "max-line-length": {
      "options": [100]
    },
    "no-console": false,
    "no-this-assignment": [true, {"allowed-names": ["^self$"], "allow-destructuring": true}],
    "no-implicit-dependencies": [true, "dev"],
    "object-literal-sort-keys": false,
    "no-object-literal-type-assertion": false,
    "typedef": [
    	true,
    	"call-signature"
    ]
  }
}
